<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jasmine.dao.admin.HonorDao">

  <resultMap id="BaseResultMap" type="cn.jasmine.entity.admin.Honor">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="honor_pic" jdbcType="VARCHAR" property="honorPic" />
    <result column="honor_title" jdbcType="VARCHAR" property="honorTitle" />
    <result column="honor_body" jdbcType="VARCHAR" property="honorBody" />
  </resultMap>


  <select id="queryAllHonor" resultMap="cn.jasmine.entity.admin.Honor">
    SELECT * FROM  honor
  </select>
</mapper>